{
    "name": "anime-db/form-type-image-bundle",
    "license": "MIT",
    "type": "library",
    "description": "Image upload form type",
    "keywords": ["php", "form", "image"],
    "homepage": "http://github.com/anime-db/form-type-image-bundle",
    "autoload": {
        "psr-4": {
            "AnimeDb\\Bundle\\FormTypeImageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnimeDb\\Bundle\\FormTypeImageBundle\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "weixiyen/jquery-filedrop",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/weixiyen/jquery-filedrop.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "weixiyen/jquery-filedrop": "dev-master"
    },
    "require-dev": {
        "symfony/symfony": "2.8.*",
        "sensio/framework-extra-bundle": "3.0.*"
    },
    "minimum-stability": "dev"
}
